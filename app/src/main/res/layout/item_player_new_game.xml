<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

	<TextView
			android:id="@+id/player_name_tv_ipng"
			style="@style/BlueDark.Size20"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginStart="16dp"
			android:layout_marginEnd="16dp"
			android:text="TextView"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/textView18"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

	<ImageView
			android:id="@+id/card_deck_icon_iv_ipng"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="16dp"
			android:layout_marginEnd="16dp"
			android:layout_marginBottom="16dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:srcCompat="@drawable/icon_card_yellow" />

	<TextView
			android:id="@+id/textView18"
			style="@style/Grey.Size20"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginEnd="16dp"
			android:text="@string/label_cards_color"
			app:layout_constraintBottom_toBottomOf="@+id/card_deck_icon_iv_ipng"
			app:layout_constraintEnd_toStartOf="@+id/card_deck_icon_iv_ipng"
			app:layout_constraintTop_toTopOf="@+id/card_deck_icon_iv_ipng" />
</androidx.constraintlayout.widget.ConstraintLayout>