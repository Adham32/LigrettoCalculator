<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

	<TextView
			android:id="@+id/value_rounds_tv_fgr"
			style="@style/Blue.Size42"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginStart="32dp"
			android:layout_marginTop="24dp"
			android:layout_marginEnd="4dp"
			android:layout_marginBottom="8dp"
			android:text="0"
			android:textAlignment="textEnd"
			android:textSize="42sp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/textView5"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

	<TextView
			android:id="@+id/textView5"
			style="@style/Blue.Size16.Uppercase"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginEnd="32dp"
			android:layout_marginBottom="12dp"
			android:text="@string/label_rounds"
			app:layout_constraintBottom_toBottomOf="@+id/value_rounds_tv_fgr"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toEndOf="@+id/value_rounds_tv_fgr" />
</androidx.constraintlayout.widget.ConstraintLayout>